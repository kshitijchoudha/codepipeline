{
  "executionRoleArn": "arn:aws:iam::800280303592:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "tf-demo-service",
      "image": "<IMAGE1_NAME>",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "tf-nginx",
  "logConfiguration": {
    "logDriver": "awslogs",
    "options": {
      "awslogs-group": "awslogs-ecs1",
      "awslogs-region": "us-east-2",
      "awslogs-stream-prefix": "awslogs-ecs"
    }
  }
}
